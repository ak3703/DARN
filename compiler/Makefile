TARFILES = Makefile scanner.mll parser.mly ast.ml darn.ml semant.ml

OBJS = ast.cmo parser.cmo scanner.cmo darn.cmo semant.cmo 

darn : $(OBJS)
	ocamlc -o darn $(OBJS)

scanner.ml : scanner.mll
	ocamllex scanner.mll

parser.ml parser.mli : parser.mly
	ocamlyacc parser.mly

%.cmo : %.ml
	ocamlc -c $<

%.cmi : %.mli
	ocamlc -c $<

.PHONY : clean
clean :
	rm -f darn parser.ml parser.mli scanner.ml *.cmo *.cmi

# Generated by ocamldep *.ml *.mli
calc.cmo: scanner.cmo parser.cmi ast.cmo
calc.cmx: scanner.cmx parser.cmx ast.cmx 
parser.cmo: ast.cmo parser.cmi 
parser.cmx: ast.cmx parser.cmi 
scanner.cmo: parser.cmi 
scanner.cmx: parser.cmx 
semant.cmo: ast.cmo
semant.cmx: ast.cmx
parser.cmi: ast.cmo 
